@{
    Layout = "/Views/Shared/_Layout.cshtml";
}

<div class="page-container">
    <div class="row">
        <div class="col text-center">
            <h1 class="display-4 py-2">Zarządzaj swoim kontem</h1>
        </div>
    </div>
    <hr />
    <div class="row">
        <div class="col-md-3">
            @await Html.PartialAsync("_ManageNav")
        </div>
        <div class="col-md-9">
            @RenderBody()
        </div>
    </div>
</div>


@section Scripts {
    @RenderSection("Scripts", required: false)
}

